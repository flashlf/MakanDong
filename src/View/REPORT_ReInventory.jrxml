	<queryString>
		<![CDATA[SELECT
     sr.`date` AS Tanggal,
     m.`Deskripsi` AS Produk,
	 sr.`Qty` AS Jumlah,
     m.`Satuan` AS Satuan,
     m.`Harga` AS Harga,
	 IF(sr.`Qty` IS NULL, 0, (sr.`Qty` * m.`Harga`)) AS SubTotal
FROM
     supplyrecord AS sr
LEFT OUTER JOIN supplier s
	ON sr.sCode = s.sCode
LEFT OUTER JOIN material m
	ON s.mCode = m.mCode
WHERE sr.sCode = $P{supplyCode} AND YEAR(sr.Date) = $P{Tahun} ORDER BY sr.date]]>
	</queryString>
